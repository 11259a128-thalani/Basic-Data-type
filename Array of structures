#include sstdio.h>
struct Student {
  int roll;
 char name[20];
};
int main() {
  struct Student s(3];
  int i;
  for (i=0;i<3;i++) {
      print("Enter roll & name: ");
      scanf("%d %s", &s[i].roll, s[i].name);
 }
