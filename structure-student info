   print("Enter roll, name and marks:");
   scanf("%d %s %f", &s.roll, s.name, &s.marks);
   printf("Student Details:\n");
   print("Roll: %d\nName: %s\nMarks: %.2f)n", s.roll, s.name,Â s.marks); 
   return 0;
}


